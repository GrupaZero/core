{
    "name": "gzero/core",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Skierniewski",
            "email": "adrian.skierniewski@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "barryvdh/laravel-cors": "^0.9.3",
        "gzero/eloquent-tree": "3.*",
        "illuminate/contracts": "5.5.*",
        "illuminate/routing": "5.5.*",
        "illuminate/support": "5.5.*",
        "laravel/passport": "^4.0",
        "robclancy/presenter": "1.3.2",
        "bkwld/croppa": "~4.0"
    },
    "require-dev": {
        "codeception/codeception": "^2.3",
        "phpunit/php-code-coverage": "5.2.*", 
        "fzaninotto/faker": "~1.6",
        "gzero/testing": "dev-master",
        "orchestra/testbench": "^3.5",
        "phpmd/phpmd": "^2.6",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^3.1",
        "mockery/mockery": "^1.0",
        "flow/jsonpath": "^0.3.4"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Gzero\\": "src/Gzero"
        }
    },
    "scripts": {
        "test": "./vendor/bin/codecept run",
        "test-debug": "./vendor/bin/codecept run --debug"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gzero\\Core\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
